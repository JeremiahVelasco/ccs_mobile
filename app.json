{
  "expo": {
    "name": "FEU CCS",
    "slug": "ccsmobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/feu_icon.png",
    "scheme": "feuccs",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "package": "com.feuccs.app",
        "foregroundImage": "./assets/images/feu_icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.ccs.feu",
      "distribution": "internal",
      "permissions": ["INTERNET", "ACCESS_NETWORK_STATE"]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/feu_icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/feu_icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "43f19679-19dd-435d-bc74-3b98129d5f38"
      }
    },
    "owner": "jeremiahvelasco"
  }
}
